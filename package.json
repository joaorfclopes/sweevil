{
  "name": "sweevil",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "backend": "nodemon --watch backend --exec node --experimental-modules backend/server.js",
    "frontend": "cd frontend && npm start",
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "build": "cd frontend && npm install && npm run build",
    "dos2unix": "find . -type f -name \"*.scss\" -exec dos2unix '{}' +",
    "deploy": "git commit --allow-empty -m \"Trigger Heroku deploy\" && npm run build && git push heroku"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaorfclopes/sweevil.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaorfclopes/sweevil/issues"
  },
  "homepage": "https://github.com/joaorfclopes/sweevil#readme",
  "dependencies": {
    "aws-sdk": "^2.1074.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.4",
    "multer": "^1.4.3",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.6.3"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "eslint-plugin-react": "^7.25.1",
    "nodemon": "^2.0.12"
  },
  "engines": {
    "node": "12.4.0",
    "npm": "6.9.0"
  }
}
